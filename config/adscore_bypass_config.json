                "chain_config": [
                    "socks5://user1:pass1@proxy1.com:1080",
                    "https://user2:pass2@proxy2.com:8080",
                    "socks5://user3:pass3@proxy3.com:1080"
                ],
                "cost": "多代理费用",
                "recommended": false
