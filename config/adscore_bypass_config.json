                "openvpn_config": {
                    "description": "OpenVPN配置示例",
                    "config_file": "client.ovpn",
                    "socks_proxy": "socks-proxy 127.0.0.1 1081"
                },
                "wireguard_config": {
                    "description": "WireGuard配置示例", 
                    "interface": "wg0",
                    "endpoint": "vpn-server.com:51820"
